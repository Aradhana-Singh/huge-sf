<apex:page controller="UpgradeQuoteWizardController" showHeader="false" sidebar="false">
  <script type="text/javascript">

    function responce(){
         var responce = "{!responce}";
         alert(responce); 
    }

  </script>

  <apex:form >
        <apex:pageBlock title="Select a Subscription">
            <apex:pageBlockButtons location="bottom" >
                <apex:commandButton value="Calculate Quote" action="{!processSelected}" oncomplete="responce()" />
            </apex:pageBlockButtons> 
          
            <apex:pageBlockTable value="{!ListFromZoura}" var="billingAccnt" id="table">
                
                <apex:column headerValue="Billing Account" >
                   <br/><apex:outputText value="{!billingAccnt.BillingAccountName}" /><br/>
                   <apex:pageblocktable value="{!billingAccnt.subList}" var="sub">
                        <apex:column headerValue="Subscription Name" >
                           <br/><apex:outputText value="{!sub.name}" /><br/>
                           
                            <apex:pageblocktable value="{!sub.ratePlanList}" var="ratePlan">
                                <apex:column >
                                       <input type="radio" name="selectedProduct" value="{!ratePlan.ratePlanId}" /> 
                                </apex:column>
                                <apex:column headerValue="Rate Plan Name" >                                  
                                   <apex:outputText value="{!ratePlan.ratePlanName}" />
                                   
                                    <!-- <apex:pageblocktable value="{!ratePlan.ratePlanCharge}" var="ratePlanCharge">
                                        <apex:column headerValue="Subscription Charge Name" >
                                           <apex:outputText value="{!ratePlanCharge.ratePlanChargeName}" />
                                        </apex:column>
                                        <apex:column headerValue="Price" >
                                           <apex:outputText value="{!ratePlanCharge.price}" />
                                        </apex:column>
                                        <apex:column headerValue="Quantity" >
                                           <apex:outputText value="{!ratePlanCharge.quantity}" />
                                        </apex:column>
                                    </apex:pageblocktable> -->
                                </apex:column>
                                <apex:column headerValue="Charge Name" >
                                    <apex:repeat value="{!ratePlan.ratePlanCharge}" var="ratePlanCharge"  >
                                          <apex:outputText value="{!ratePlanCharge.ratePlanChargeName}" />
                                    </apex:repeat> 
                                </apex:column>
                                <apex:column headerValue="Price" >
                                    <apex:repeat value="{!ratePlan.ratePlanCharge}" var="ratePlanCharge"  >
                                          <apex:outputText value="{!ratePlanCharge.price}" />
                                    </apex:repeat> 
                                </apex:column>
                                <apex:column headerValue="Quantity" >
                                    <apex:repeat value="{!ratePlan.ratePlanCharge}" var="ratePlanCharge"  >
                                          <apex:outputText value="{!ratePlanCharge.quantity}"/>
                                    </apex:repeat> 
                                </apex:column>
                                <apex:column headerValue="Additional Users">
                                    <apex:inputText value="{!ratePlan.additionalUsers}" />                                       
                                </apex:column>
                            </apex:pageblocktable>
                           
                        </apex:column>  
                        <!-- <apex:column headerValue="Subscription Start Date" >
                           <apex:outputText value="{!sub.termStartDate}" />
                        </apex:column>
                         <apex:column headerValue="Subscription End Date" >
                           <apex:outputText value="{!sub.termEndDate}" />
                        </apex:column> -->
                   </apex:pageblocktable>
                </apex:column>          
               
            </apex:pageBlockTable>
        </apex:pageBlock>
    </apex:form>
</apex:page>