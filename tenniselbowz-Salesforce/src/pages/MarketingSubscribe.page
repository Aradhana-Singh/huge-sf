<apex:page controller="MarketingSubscribeController" standardStylesheets="FALSE" showHeader="FALSE">
  <script>
  function confirmCancel() {
      var isCancel = confirm("Are you sure you wish to cancel?");
      if (isCancel) return true;
  
     return false;
  }  
  </script>
  <style>
.test1{
width:500px;
height:500px;

}


</style>
  <apex:stylesheet value="{!URLFOR($Resource.NASubscriberForm)}"/>
  
  <apex:form >
      <apex:pageBlock mode="edit">
          <apex:pageBlockButtons >
              <apex:commandButton action="{!save}" value="Submit"/>
              <apex:commandButton action="{!cancel}" value="Cancel" onclick="return confirmCancel()"/>
          </apex:pageBlockButtons>
          <apex:pageblockSection title="Subscriber Data" collapsible="fasle" columns="2">
              <apex:outputPanel rendered="{!ShowSuccessMessages}" >
                  <div class="text">
                
                    Your Subscription Information was Saved Successfully.
                  </div>
              </apex:outputPanel>
              <apex:pageBlockSectionItem DAtaStyleClass="test1">
              <apex:outputPanel Id="FormDetail" rendered="{!!ShowSuccessMessages}" style="width:50%" >
                  
                               
                    <div class="cfg-element-container">
                        <label class="inline-inputlabel" ><span class="cfg-label-value">First Name</span><span class="redSuperScript">*</span></label>
                            <apex:inputField value="{!SubscriberContact.FirstName}" id="firstName" required="TRUE" styleclass="inline-form-control" html-placeholder="Enter First Name." />
                    </div>
                                                        
                    <div class="cfg-element-container">
                        <label class="inline-inputlabel" ><span class="cfg-label-value">Last Name</span><span class="redSuperScript">*</span></label>
                            <apex:inputField value="{!SubscriberContact.LastName}" id="LastName" required="TRUE" styleclass="inline-form-control" html-placeholder="Enter Last Name." />
                    </div>
                    
                    <div class="cfg-element-container">
                        <label class="inline-inputlabel" ><span class="cfg-label-value">Email</span><span class="redSuperScript">*</span></label>
                            <apex:inputField value="{!SubscriberContact.Email}" id="Email" required="TRUE" styleclass="inline-form-control" html-placeholder="Enter Email." />
                    </div>
                    
                    <div class="cfg-element-container">
                        <label class="inline-inputlabel" ><span class="cfg-label-value">Company Name</span><span class="redSuperScript">*</span></label>
                            <apex:inputField value="{!SubscriberAccount.name}" id="CompanyName" required="TRUE" styleclass="inline-form-control" html-placeholder="Enter Company Name." />
                    </div>
                                                          
                    <div class="cfg-element-container">
                        <label class="inline-inputlabel" ><span class="cfg-label-value">JobTitle</span><span class="redSuperScript">*</span></label>
                            <apex:inputField value="{!SubscriberContact.Subscription_Job_Title__c}" id="JobTitle" required="TRUE" styleclass="inline-form-control" html-placeholder="Enter Job Title." />
                    </div>
                    
                    <div class="cfg-element-container">
                        <label class="inline-inputlabel" ><span class="cfg-label-value">Country</span><span class="redSuperScript">*</span></label>
                            <apex:inputField value="{!SubscriberAccount.BillingCountryCode}" id="Country" required="TRUE" styleclass="inline-form-control" html-placeholder="Select a Country." />
                    </div>
                    
                     <div class="cfg-element-container">
                        <label class="inline-inputlabel" ><span class="cfg-label-value">State</span><span class="redSuperScript">*</span></label>
                            <apex:inputField value="{!SubscriberAccount.BillingStateCode}" id="State" required="TRUE" styleclass="inline-form-control" html-placeholder="Select State." />
                    </div>
                    
                    <div class="cfg-element-container">
                        <label class="inline-inputlabel" ><span class="cfg-label-value">Postal Code</span><span class="redSuperScript">*</span></label>
                            <apex:inputField value="{!SubscriberAccount.BillingPostalCode}" id="PostalCode" required="TRUE" styleclass="inline-form-control" html-placeholder="Enter Postal Code." />
                    </div>
                    
                    <div class="cfg-element-container">
                        <label class="inline-inputlabel" ><span class="cfg-label-value">Address</span></label><br></br>
                            <apex:inputField value="{!SubscriberAccount.BillingStreet}" id="street" required="FALSE" styleclass="inline-form-control" html-placeholder="Enter Street Address." />
                    </div>
                    
                    
                     <div class="cfg-element-container">
                        <label class="inline-inputlabel" ><span class="cfg-label-value">City</span></label><br></br>
                            <apex:inputField value="{!SubscriberAccount.BillingCity}" id="City" required="FALSE" styleclass="inline-form-control" html-placeholder="Enter City name." />
                    </div>
                    
                    <div class="cfg-element-container">
                        <label class="inline-inputlabel" ><span class="cfg-label-value">Phone</span></label><br></br>
                            <apex:inputField value="{!SubscriberContact.Phone}" id="Phone" required="FALSE" styleclass="inline-form-control" html-placeholder="Enter Phone Number." />
                    </div>
                    </apex:outputPanel>
                    </apex:pageBlockSectionItem>
                    
                    <apex:pageBlockSectionItem dataStyle="vertical-align:top" labelTitle="Subscriptions" >
                    <apex:outputPanel title="Subscriptions">
                    <b>I would like to receive the following subscriptions:</b><br/><br/>
                    <div>
                        <label for = "AppliedSystemsToolbox " style = "font-weight: normal">Applied Systems Toolbox</label>
                            <apex:inputField value="{!SubscriberContact.Subscription_Applied_Systems_Toolbox__c}" id="AppliedSystemsToolbox" required="FALSE" styleclass="inline-inputlabel" />
                    </div>
                    
                    <div >
                        <label for = "eQuestUpdates" style = "font-weight: normal">eQuest Updates</label>
                            <apex:inputField value="{!SubscriberContact.Subscription_eQUEST_Updates__c}" id="eQuestUpdates" required="FALSE" styleclass="inline-inputlabel"  />
                    </div>
                    
                     <div >
                        <label for = "NewsCompanyIndustryTech" style = "font-weight: normal">NEWS Company Industry Tech</label>
                            <apex:inputField value="{!SubscriberContact.Subscription_News_Company_Industry_Tech__c}" id="NewsCompanyIndustryTech" required="FALSE" styleclass="inline-inputlabel" />
                    </div>
                    
                    <div >
                        <label for = "RevitUpdates" style = "font-weight: normal">Revit Updates</label>
                            <apex:inputField value="{!SubscriberContact.Subscription_Revit_Updates__c}" id="RevitUpdates" required="FALSE" styleclass="inline-inputlabel" />
                    </div>
                    </apex:outputPanel>
                    </apex:pageBlockSectionItem>
