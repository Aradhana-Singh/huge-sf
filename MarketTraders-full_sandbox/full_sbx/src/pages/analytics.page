<apex:page showHeader="false" standardStyleSheets="false" controller="Internal_Webinars" readOnly="true">
    <apex:composition template="composition">
        <apex:define name="page_title">Anayltics</apex:define>
        <apex:define name="breadcrumb">
            <ul class="breadcrumb">
                <li>
                    <i class="icon-home"></i>
                    <apex:outputLink value="dashboard">Home</apex:outputLink>
                    <i class="icon-angle-right"></i>
                </li>
                <li><apex:outputLink value="#">Analytics</apex:outputLink></li>
            </ul>
        </apex:define>
        <apex:define name="content">

            <apex:dataTable value="{!map_webinars}" var="webinar" id="table-webinars" styleClass="table table-striped">
                <apex:column >
                    <apex:facet name="header">Webinar ID</apex:facet>
                    <apex:outputText value="{!map_webinars[webinar].webinar_id}"/>
                </apex:column>
                <apex:column >
                    <apex:facet name="header">Redirect Visit Amount</apex:facet>
                    <apex:outputText value="{!map_webinars[webinar].visit_count}"/>
                </apex:column>
                <apex:column >
                    <apex:facet name="header">Webinar Attendance Amount</apex:facet>
                    <apex:outputText value="{!map_webinars[webinar].attended_count}"/>
                </apex:column>
                 <apex:column >
                    <apex:facet name="header">Start Time</apex:facet>
                    <apex:outputField value="string: {0,date,yyyy.MM.dd G 'at' HH:mm:ss z}"> <apex:param value="{!map_webinars[webinar].start_time}" /> </apex:outputField>
                </apex:column>
                <!--
                <apex:column >
                    <apex:facet name="header">End Time</apex:facet>
                    <apex:outputField value="{!map_webinars[webinar].end_time}"/>
                </apex:column>
				-->
            </apex:dataTable>

        </apex:define>
    </apex:composition>
</apex:page>