<apex:page controller="InternalClass">
    <apex:include pageName="head"/>
    <body class="page-header-fixed">
    <apex:include pageName="header"/>
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
    
        <apex:include pageName="sidebar"/>
    
        <!-- BEGIN PAGE -->
        <div class="page-content">
    
            <!-- BEGIN PAGE CONTAINER-->
            <div class="container-fluid">

                <!-- BEGIN PAGE HEADER-->
                <div class="row-fluid">
                    <div class="span12">
                        <!-- BEGIN PAGE TITLE & BREADCRUMB-->
                        <h3 class="page-title">
                            <apex:insert name="page_title"/>
                        </h3>
                        <apex:insert name="breadcrumb"/>
                        <!-- END PAGE TITLE & BREADCRUMB-->
                    </div>
                </div>
                <!-- END PAGE HEADER-->

                <!-- BEING PAGE CONTENT -->
                <apex:insert name="content"/>
                <!-- END PAGE CONTENT -->
            </div>
            <!-- END PAGE CONTAINER-->
        </div>
        <!-- END PAGE -->
    </div>
    <!-- END CONTAINER -->
    <apex:include pageName="footer"/>
    </body>
</apex:page>